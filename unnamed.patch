+                if (details.containsFaults) {
+                    Spacer(modifier = Modifier.height(8.dp))
+                    Text("Fault Codes:", color = Color.Red)
+                    details.faults.forEachIndexed { index, code ->
+                        if (code != 0.toShort()) {
+                            Text("Slot ${index + 1}: 0x${code.toString(16).uppercase()}")
+                        }
+                    }
+                }
+            }
+        }
+    }
