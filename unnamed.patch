+                    Text("Status:")
+                    Text(
+                        if (details.containsFaults) "FAULT DETECTED" else "OK",
+                        color = if (details.containsFaults) Color.Red else Color.Green,
+                        fontWeight = FontWeight.Bold
+                    )
+                }
